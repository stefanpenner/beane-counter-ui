<table class="metrics">
  <thead>
    <tr>
      {{#each headers}}
        <th>{{this}}</th>
      {{/each}}
    </tr>
  </thead>
  <tbody>
    {{#each entries}}
      <tr>
        <td>{{team}}</td>
        <td class="value">
          {{wins}}
        </td>
        <td class="value">
          {{losses}}
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
